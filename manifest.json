{
  "name": "Kiddy",
  "version": "1.0.0",
  "description": "Web Browser for Kids",
  "manifest_version": 3,
  "author": "Afa <ifundeasy@gmail.com>",
   "icons": {
    "16": "res/icon16.png",
    "32": "res/icon32.png",
    "48": "res/icon48.png",
    "128": "res/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["script.js"]
    }
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
